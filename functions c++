//learning functions in c++
//question- power(a,b)
//method 1
#include <iostream>
using namespace std;
int power(int a,int b){
    int ans=1;
    while(b!=0){
        ans=ans*a;
        b=b-1;
    }
}
int main() {
    // Write C++ code here
    int base,exponent;
    cin>>base>>exponent;
    int answer=power(base,exponent);
    cout<<answer;
    return 0;
}
//method 2
int power(int a,int b){
    int ans=1;
    for(int i=1;i<=b;i++)
        ans=ans*a;
        return ans;
}
//even odd function question
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

bool isEven(int a){
    if(a&1){
        return 0;
    }
    return 1;
}
int main(){

    int a;
    cin>>a;
    int sol=isEven(a);
    if(sol){
        cout<<"is even";
    }
    else{
        cout<<"is odd";
    }
}
