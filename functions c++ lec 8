//learning functions in c++
//question- power(a,b)
//method 1
#include <iostream>
using namespace std;
int power(int a,int b){
    int ans=1;
    while(b!=0){
        ans=ans*a;
        b=b-1;
    }
}
int main() {
    // Write C++ code here
    int base,exponent;
    cin>>base>>exponent;
    int answer=power(base,exponent);
    cout<<answer;
    return 0;
}
//method 2
int power(int a,int b){
    int ans=1;
    for(int i=1;i<=b;i++)
        ans=ans*a;
        return ans;
}
//even odd function question
// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

bool isEven(int a){
    if(a&1){
        return 0;
    }
    return 1;
}
int main(){

    int a;
    cin>>a;
    int sol=isEven(a);
    if(sol){
        cout<<"is even";
    }
    else{
        cout<<"is odd";
    }
}
//code for nCr
#include <iostream>
using namespace std;

int fact(int n){
    int ans=1;
    while(n!=0){
        ans=ans*n;
        n=n-1;
    }
    return ans;
}
int nCr(int n,int r){
    int num= fact(n);
    int denom= fact(r)*fact(n-r);
    return num/denom;
}
int main(){
    int n,r;
    cin>>n>>r;
    int sol= nCr(n,r);
    cout<<sol;
}
//print counting program
#include<iostream>
#include<math.h>
using namespace std;
void printCounting(int n){
    for(int i=1;i<=n;i++){
        cout<<i<<endl;
    }
}
int main(){
    int k;
    cin>>k;
    printCounting(k);
    
}
// prime number checker using functions
#include<iostream>
#include<math.h>
using namespace std;
bool primeNumber(int n){
    
    for(int i=2;i<n;i++){
        if(n%i==0)
            return 0;
    }
    return 1;
}
int main(){
    int k,res;
    cin>>k;
    res= primeNumber(k);
    if(res){
        cout<<"is prime";
    }
    else
    cout<<"not prime";
}
